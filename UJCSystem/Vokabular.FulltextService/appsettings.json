{
  "Endpoints": [
    {
      "Name": "ElasticSearchService",
      "Address": "http://localhost:9200"
    }
  ],
  "ElasticsearchIndices": {
    "PageIndex": "pageindex",
    "SnapshotIndex": "snapshotindex"
  },
  "SpecialChars": {
    "EscapeCharacter": "\\",
    "CommentMarkOpening": "$",
    "CommentMarkClosing": "%",
    "EscapedEscapeCharacter": "\\\\",
    "EscapedCommentMarkOpening": "\\$",
    "EscapedCommentMarkClosing": "\\%"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
