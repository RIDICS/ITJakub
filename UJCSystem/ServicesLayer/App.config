<?xml version="1.0"?>
<configuration>

  <appSettings>
  </appSettings>
  
  <system.web>
    <compilation debug="true"/>
  </system.web>
  <!-- When deploying the service library project, the content of the config file must be added to the host's 
  app.config file. System.Configuration does not support config files for libraries. -->
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="ServicesLayer.ValidationServiceBehavior"
        name="ServicesLayer.ValidationService">
        <endpoint address="" binding="wsHttpBinding" contract="ServicesLayer.ValidationService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8732/Design_Time_Addresses/ServicesLayer/ValidationService/" />
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="ServicesLayer.ExtractServiceBehavior"
        name="ServicesLayer.AdvFilesService">
        <endpoint address="" binding="wsHttpBinding" contract="ServicesLayer.ICompressService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8732/Design_Time_Addresses/ServicesLayer/ExtractService/" />
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="ServicesLayer.LogServiceBehavior"
        name="ServicesLayer.LogService">
        <endpoint address="" binding="wsHttpBinding" contract="ServicesLayer.ILogService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8732/Design_Time_Addresses/ServicesLayer/LogService/" />
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="ServicesLayer.LangServiceBehavior"
        name="ServicesLayer.LangService">
        <endpoint address="" binding="wsHttpBinding" contract="ServicesLayer.ILangService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8732/Design_Time_Addresses/ServicesLayer/LangService/" />
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="ServicesLayer.AuthServiceBehavior"
        name="Ujc.Naki.ServicesLayer.AuthService">
        <endpoint address="" binding="wsHttpBinding" contract="Ujc.Naki.ServicesLayer.IAuthService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8732/Design_Time_Addresses/ServicesLayer/AuthService/" />
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="ServicesLayer.SearchServiceBehavior"
        name="ServicesLayer.SearchService">
        <endpoint address="" binding="wsHttpBinding" contract="Ujc.Naki.ServicesLayer.SearchService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8732/Design_Time_Addresses/ServicesLayer/FindService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ServicesLayer.ValidationServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
        <behavior name="ServicesLayer.ExtractServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
        <behavior name="ServicesLayer.LogServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
        <behavior name="ServicesLayer.LangServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
        <behavior name="ServicesLayer.AuthServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
        <behavior name="ServicesLayer.SearchServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
