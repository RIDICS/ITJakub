<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping assembly="ITJakub.MobileApps.DataEntities" namespace="ITJakub.MobileApps.DataEntities.ExternalEntities.SqlServer.Entities"
                   xmlns="urn:nhibernate-mapping-2.2">
  <class name="SynchronizedObjectData" table="[SynchronizedObjectData]">

    <id name="Id" column="Id">
      <generator class="identity" />
    </id>

    <many-to-one name="Group" lazy="proxy" column="[Group]" 
                 class="ITJakub.MobileApps.DataEntities.Database.Entities.Group, ITJakub.MobileApps.DataEntities" />

    <property name="Data" type="StringClob">
      <column name="Data" sql-type="nvarchar(max)" not-null ="true"/>
    </property>

  </class>
</hibernate-mapping>