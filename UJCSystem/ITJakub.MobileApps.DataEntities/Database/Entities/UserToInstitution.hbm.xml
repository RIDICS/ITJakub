<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.MobileApps.DataEntities" namespace="ITJakub.MobileApps.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="UserToInstitution" table="UserToInstitution" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one name="User">
      <column name="UserId" sql-type="bigint" not-null="true" />
    </many-to-one>
    <many-to-one name="Institution">
      <column name="InstitutionId" sql-type="bigint" not-null="true" />
    </many-to-one>
  </class>
</hibernate-mapping>