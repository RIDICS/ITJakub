<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.MobileApps.DataEntities" namespace="ITJakub.MobileApps.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Application" table="[Application]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="varchar" not-null="true" />
    </property>
    <bag name="Tasks">
      <key column="ApplicationId" />
      <one-to-many class="Task" />
    </bag>
  </class>
</hibernate-mapping>