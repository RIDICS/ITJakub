<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.MobileApps.DataEntities" namespace="ITJakub.MobileApps.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Sysdiagrams" table="sysdiagrams" lazy="true" >
    <id name="DiagramId" column="diagram_id">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="name" sql-type="nvarchar" not-null="true" unique="true" />
    </property>
    <property name="PrincipalId">
      <column name="principal_id" sql-type="int" not-null="true" unique="true" />
    </property>
    <property name="Version">
      <column name="version" sql-type="int" not-null="false" />
    </property>
    <property name="Definition">
      <column name="definition" sql-type="varbinary" not-null="false" />
    </property>
  </class>
</hibernate-mapping>