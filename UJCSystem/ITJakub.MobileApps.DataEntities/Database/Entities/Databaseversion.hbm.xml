<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.MobileApps.DataEntities" namespace="ITJakub.MobileApps.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Databaseversion" table="DatabaseVersion" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Databaseversion">
      <column name="DatabaseVersion" sql-type="varchar" not-null="true" />
    </property>
    <property name="Solutionversion">
      <column name="SolutionVersion" sql-type="varchar" not-null="false" />
    </property>
    <property name="Upgradedate">
      <column name="UpgradeDate" sql-type="datetime" not-null="true" />
    </property>
    <property name="Upgradeuser">
      <column name="UpgradeUser" sql-type="varchar" not-null="true" />
    </property>
  </class>
</hibernate-mapping>