<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.MobileApps.DataEntities" namespace="ITJakub.MobileApps.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="UserToGroup" table="UserToGroup" lazy="true" >
    <composite-id>
      <key-property name="UserId" column="UserId" />
      <key-property name="GroupId" column="GroupId" />
    </composite-id>
    <many-to-one name="User">
      <column name="UserId" sql-type="bigint" not-null="true" />
    </many-to-one>
    <many-to-one name="Group">
      <column name="GroupId" sql-type="bigint" not-null="true" />
    </many-to-one>
  </class>
</hibernate-mapping>