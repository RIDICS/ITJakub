      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost/ITJakub.SearchService/SearchService.svc" name="BasicHttpBinding_ISearchService" bindingConfiguration="BasicHttpBinding_ITJakub" binding="basicHttpBinding" contract="ITJakub.SearchService.DataContracts.ISearchService" />
