<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.DataEntities" namespace="ITJakub.DataEntities.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="AuthorInfo" table="[AuthorInfo]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one name="Author">
      <column name="Author" sql-type="int" not-null="true" />
    </many-to-one>
    <property name="Text">
      <column name="Text" sql-type="varchar" not-null="true" />
    </property>
    <property name="TextType">
      <column name="TextType" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>