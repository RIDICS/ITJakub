<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.DataEntities" namespace="ITJakub.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ManuscriptDescription" table="[ManuscriptDescription]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one name="BookVersion">
      <column name="BookVersion" sql-type="bigint" not-null="false" />
    </many-to-one>
    <property name="Title">
      <column name="Title" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Idno">
      <column name="Idno" sql-type="varchar" not-null="false"/>
    </property>   
    <property name="Settlement">
      <column name="Settlement" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Country">
      <column name="Country" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Repository">
      <column name="Repository" sql-type="varchar" not-null="false" />
    </property>
    <property name="OriginDate">
      <column name="OriginDate" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>
