<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.DataEntities" namespace="ITJakub.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="BookBibl" table="[BookBibl]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one name="BookVersion">
      <column name="BookVersion" sql-type="bigint" not-null="true" />
    </many-to-one>
    <property name="Text">
      <column name="Text" sql-type="varchar" not-null="false" />
    </property>
    <property name="Type">
      <column name="Type" sql-type="varchar" not-null="false" />
    </property>
    <property name="SubType">
      <column name="Subtype" sql-type="varchar" not-null="false" />
    </property>
    <property name="BiblType">
      <column name="BiblType" sql-type="smallint" not-null="false" />
    </property>
  </class>
</hibernate-mapping>