<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ITJakub.DataEntities" namespace="ITJakub.DataEntities.Database.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Image" table="[Image]" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <many-to-one name="Book">
      <column name="Book" sql-type="bigint" not-null="true" />
    </many-to-one>
    <property name="FileName">
      <column name="FileName" sql-type="varchar" not-null="true" />
    </property>
    <property name="ImageType">
      <column name="ImageType" sql-type="smallint" not-null="true" />
    </property>
  </class>
</hibernate-mapping>