                Id: 9,
                Name: "Tag1",
                Color: "#FF1010"
            }
        },
        {
            Id: 5,
            Title: "První item",
            FavoriteLabel: {
                Id: 91,
                Name: "Tag2",
                Color: "#10FF10"
            }
        }]);

        favStar.addFavoriteLabels([{
            Id: 91,
            Name: "Tag2",
            Color: "#10FF10"
        },
        {
            Id: 9,
            Name: "Tag1",
            Color: "#FF1010"
        }]);

        favStar.make();
    });
</script>