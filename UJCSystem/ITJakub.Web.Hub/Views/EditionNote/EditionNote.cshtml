@using Microsoft.AspNetCore.Html
@using Scalesoft.Localization.AspNetCore

@inject ILocalizationService Localizer

@{
    ViewBag.Title = Localizer.Translate("EditionNote", "EditionNote");
}

<div id="edition-note-content" class="content-container top-offset-without-submenu">
    <h1>@ViewBag.Title</h1>

    <p>
    @(new HtmlString(ViewData["noteText"].ToString()))
    </p>
</div>
