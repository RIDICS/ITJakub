@using Localization.AspNetCore.Service
@model ITJakub.Web.Hub.Models.FeedbackViewModel
@inject ILocalization Localizer

@section styles{
    <link href="~/Areas/Bibliographies/css/itjakub.bibliographies.css" rel="stylesheet" />
}

@{
    ViewBag.Title = @Localizer.Translate("BibliographiesFeedback", "Bibliographies");
}

<div class="content-container">
    @Html.Partial("_StaticText", Model.PageStaticText)

    @Html.Partial("Feedback/_FeedbackForm", Model)
</div>
