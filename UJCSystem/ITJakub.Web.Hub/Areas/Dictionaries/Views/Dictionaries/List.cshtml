@using Scalesoft.Localization.AspNetCore
@inject ILocalizationService Localizer

@section styles{
    <link asp-append-version="true" href="~/Areas/Dictionaries/css/itjakub.dictionaries.css" rel="stylesheet" />
}
@section scripts{
    <script asp-append-version="true" src="~/Areas/Dictionaries/js/itjakub.dictionaries.list.js"></script>
}

@{
    ViewBag.Title = @Localizer.Translate("DictionariesList", "Dictionaries");
}

<script type="text/javascript">
    $(document).ready(function() {
        initDictionaryList();
    });
</script>

@await Html.PartialAsync("_BibliographySearch")
