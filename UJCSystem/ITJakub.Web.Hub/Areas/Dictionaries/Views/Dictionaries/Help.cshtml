@inject ILocalizationService Localizer

@using Scalesoft.Localization.AspNetCore
@model ITJakub.Web.Hub.Models.StaticTextViewModel
@section styles{
    <link asp-append-version="true" href="~/Areas/Dictionaries/css/itjakub.dictionaries.css" rel="stylesheet" />
}
@{
    ViewBag.Title = @Localizer.Translate("DictionariesHelp", "Dictionaries");
}

<div class="content-container">
    @await Html.PartialAsync("_StaticText", Model)
</div>