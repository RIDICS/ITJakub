  "Default": {
    "middle-panel": {
      "title": "{$withoutEmpty}",
      "body": "{AuthorsLabel}",
      "variables": {
        "$authors": {
          "type": "array",
          "scope": "Authors",
          "pattern": "{Name}",
          "delimeter": ", "
        },
        "$withoutEmpty": {
          "type": "replace",
          "pattern": "{Name},,,,",
          "replacing": ",{2,}",
          "replacement": " "
        }
      }
    },
    "right-panel": {

    }
  },
  "AudioBook": {
    "middle-panel": {
      "title": "{$printAuthors}{Title}",
      "body": "{PublishPlace}, {PublishDate}, {PublisherText} <br> {$manuscript} <br> Stáhnout celou audioknihu: {$fullRecords}",
      "favorites": {
        "maxCount": 5
      },
      "variables": {
        "$manuscript": {
          "type": "basic",
          "pattern": "{ManuscriptSettlement}, {ManuscriptRepository}, {OriginDate}",
          "printIfNullValue": false
        },
        "$printAuthors": {
          "type": "basic",
          "pattern": "{AuthorsLabel}: ",
          "printIfNullValue": false
        },
        "$authors": {
          "type": "array",
          "scope": "Authors",
          "pattern": "{Name}",
          "delimeter": ", "
        },
        "$fullRecords": {
          "type": "array",
          "scope": "FullBookRecordings",
          "pattern": "<a href='DownloadAudio?audioId={Id}&audioType={AudioType}' class='audio-download-href'>{$translateAudioType}</a>",
          "printIfNullValue": false,
          "delimeter": " "
        },
        "$translateAudioType": {
          "type": "script",
          "pattern": "translateAudioType(\"{AudioType}\");",
          "printIfNullValue": false
        }
      }
    },
    "right-panel": {
      "info-button": {
        "url": "AudioBooks\\AudioBooks\\Listing?bookId={BookXmlId}"
      },
      "favorite-button": {},
      "load-detail-url": "Bibliography/GetAudioBookDetailInfo?bookId={BookId}"
    },
    "bottom-panel": {
      "body": "{$tracksList}",
      "variables": {
        "$tracksList": {
          "type": "basic",
          "pattern": "<ol class='tracks-list'>{$tracks}</ol>",
          "printIfNullValue": false
        },
        "$tracks": {
          "type": "array",
          "scope": "Tracks",
          "pattern": "<li class='track'>{Name}<div class='audio'><div class='audio-text'>{Text}</div><audio controls='controls' preload='none'>{$trackRecordings}Váš prohlížeč nepodporuje html audio<code>audio</code></audio><div class='audio-length'>Délka: <div class='audio-length-value'>{$trackLength}</div></div><div class='audio-download'>Stáhnout kapitolu: {$trackRecordingsDownload}</div></div></li>",
          "delimeter": ""
        },
        "$trackLength": {
          "type": "basic",
          "scope": "Recordings",
          "pattern": "{$firstTrack}"
        },
        "$firstTrack": {
          "type": "basic",
          "scope": "0",
          "pattern": "{$printLength}"
        },
        "$printLength": {
          "type": "script",
          "scope": "Duration",
          "pattern": "getAudioLengthString(\"{$this}\")"
        },
        "$trackRecordings": {
          "type": "array",
          "scope": "Recordings",
          "pattern": "<source src='DownloadAudio?audioId={Id}&audioType={AudioType}' type='{MimeType}'>",
          "delimeter": ""
        },
        "$trackRecordingsDownload": {
          "type": "array",
          "scope": "Recordings",
          "pattern": "<a href='DownloadAudio?audioId={Id}&audioType={AudioType}' class='audio-download-href'>{$translateAudioType}</a>",
          "delimeter": " "
        },
        "$translateAudioType": {
          "type": "script",
          "pattern": "translateAudioType(\"{AudioType}\");",
          "printIfNullValue": false
        }
      }
    }
  },
