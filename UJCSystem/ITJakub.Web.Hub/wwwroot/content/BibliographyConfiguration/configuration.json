{
  "Edition": {
    "middle-panel": {
      "title": "{$printAuthors}{Title}",
      "body": "{PublishPlace}, {PublishDate}, {PublisherText} <br> {$manuscript} <br> {@NumberOfPages}: {PageCount}",
      "favorites": {
        "maxCount": 5
      },
      "variables": {
        "$test": {
          "type": "if",
          "pattern": "{Century}",
          "onTrue": "{Editor}",
          "onFalse": "was false"
        },
        "$manuscript": {
          "type": "basic",
          "pattern": "{ManuscriptSettlement}, {ManuscriptRepository}, {OriginDate}",
          "printIfNullValue": false
        },
        "$printAuthors": {
          "type": "basic",
          "pattern": "{AuthorsLabel}: ",
          "printIfNullValue": false
        },
        "$authors": {
          "type": "array",
          "scope": "Authors",
          "pattern": "{Name}",
          "delimeter": ", "
        },
        "$litType": {
          "type": "array",
          "scope": "Pages",
          "pattern": "<a href='{BookXmlId}'>[{$LiteraryTypeStart}{$LiteraryTypeEnd}]</a>",
          "delimeter": " , "
        },
        "$LiteraryTypeStart": {
          "type": "basic",
          "pattern": "{Start}",
          "printIfNullValue": false,
          "replaceNullValueBy": ""
        },
        "$LiteraryTypeEnd": {
          "type": "basic",
          "pattern": "-{End}",
          "printIfNullValue": false,
          "replaceNullValueBy": ""
        },
        "$sign": {
          "type": "basic",
          "pattern": ", {Sign}",
          "printIfNullValue": false,
          "replaceNullValueBy": ""
        },
        "$archive": {
          "type": "basic",
          "scope": "Archive",
          "pattern": " {Name} ({City}, {State})",
          "printIfNullValue": false
        },
        "$century": {
          "type": "basic",
          "pattern": ", {Century}. {@Century}",
          "printIfNullValue": false
        },
        "$pages": {
          "type": "basic",
          "pattern": ", 5v- 6v, 11v-12v ",
          "printIfNullValue": false
        }
      }
    },
    "middle-right-panel": {
      "body": "<img src='/images/logo/logo-bib.{ProjectType}.png' title='{ProjectTypeString}'>"
    },
    "bottom-panel": {
      "body": "{$printSearchResultsCondition}{$editionTable}<a href='/EditionNote/EditionNote?bookId={BookId}'>{@EditionNote}</a> <br> {@Copyright}: {Copyright}",
      "variables": {
        "$editionTable": {
          "type": "table",
          "printRowIfNullValue": true,
          "replaceNullValueBy": "NotFilled",
          "rows": [
            {
              "label": "Editor",
              "pattern": "{$editor}"
            },
            {
              "label": "Pattern",
              "pattern": "{$literaryOriginal}"
            },
            {
              "label": "RelicAbbrevitation",
              "pattern": "{RelicAbbreviation}"
            },
            {
              "label": "SourceAbbrevitation",
              "pattern": "{SourceAbbreviation}"
            },
            {
              "label": "LiteraryType",
              "pattern": "{$literaryType}"
            },
            {
              "label": "LiteraryGenre",
              "pattern": "{$literaryGenre}"
            },
            {
              "label": "EditionLastModification",
              "pattern": "{CreateTimeString}"
            },
            {
              "label": "LinkToForum",
              "pattern": "{$forumLink}"
            }
          ]
        },
        "$printSearchResults": {
          "type": "basic",
          "pattern": "<div class=\"results\"><div class=\"hits-count\">({TotalHitCount}x)</div><div class=\"hits-results\">{$printSearchResultsArray}</div></div>",
          "printIfNullValue": true
        },
        "$printSearchResultsArray": {
          "type": "array",
          "scope": "Results",
          "pattern": "<div class=\"result-hit\">{$printSearchResultContext}</div>",
          "delimeter": ""
        },
        "$printSearchResultContext": {
          "type": "basic",
          "scope": "ContextStructure",
          "pattern": "<div class=\"search-context-before\">{Before}</div> <div class=\"search-context-match\">{Match}</div><div class=\"search-context-after\"> {After}</div>",
          "delimeter": ""
        },
        "$printSearchResultsCondition": {
          "type": "if",
          "pattern": "{Results}",
          "onTrue": "{$printSearchResults}",
          "onFalse": ""
        },

        "$editor": {
          "type": "array",
          "scope": "Editors",
          "pattern": "{FirstName} {LastName}",
          "delimeter": ", "
        },
        "$literaryOriginal": {
          "type": "array",
          "scope": "LiteraryOriginals",
          "pattern": "{$this}",
          "delimeter": ", "
        },
        "$literaryType": {
          "type": "array",
          "scope": "LiteraryKinds",
          "pattern": "{$this}",
          "delimeter": ", "
        },
        "$literaryGenre": {
          "type": "array",
          "scope": "LiteraryGenres",
          "pattern": "{$this}",
          "delimeter": ", "
        },
        "$forumLink": {
          "type": "basic",
          "pattern": "<a href='{ForumUrl}'>{@Link}</a>",
          "printIfNullValue": false
        }
      }
    },
    "right-panel": {
      "read-button": {
        "url": "Editions\\Editions\\Listing?bookId={BookId}"
      },
      "favorite-button": {},
      "load-detail-url": "Bibliography/GetBookDetailInfo?bookId={BookId}",
      "variables": {
        "$test": {
          "type": "basic",
          "pattern": "{Name}",
          "printIfNullValue": false
        }
      }

    }
  },
  "Dictionary": {
    "middle-panel": {
      "short-info": "{$linked}",
      "title": "{Title} <span class=\"acronym\">[{SourceAbbreviation}]</span>",
      "body": "{BiblText} <br> {PublishPlace}, {PublishDate} <br> {$authors}{$description}{$year}",
      "favorites": {
        "maxCount": 5
      },
      "variables": {
        "$linked": {
          "type": "if",
          "pattern": "{Linked}",
          "onTrue": "LinkedDictionary",
          "onFalse": "UnlinkedDictionary"
        },
        "$authors": {
          "type": "basic",
          "pattern": "{AuthorsLabel}",
          "printIfNullValue": false
        },
        "$description": {
          "type": "basic",
          "pattern": ", {Description}",
          "printIfNullValue": false
        },
        "$year": {
          "type": "basic",
          "pattern": " {OriginDate}.",
          "printIfNullValue": false
        }
      }
    },
    "middle-right-panel": {
      "body": "<img src='/images/logo/logo-bib.{ProjectType}.png' title='{ProjectTypeString}'>"
    },
    "right-panel": {
      "read-button": {
        "url": "Dictionaries\\Dictionaries\\Listing?books=[{BookId}]"
      },
      "favorite-button": {},
      "load-detail-url": "Bibliography/GetBookDetailInfo?bookId={BookId}"
    }
  },

