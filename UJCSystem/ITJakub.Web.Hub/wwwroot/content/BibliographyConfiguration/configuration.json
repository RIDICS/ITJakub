  "CardFile": {
    "middle-panel": {
      "title": "{Title}",
      "body": "{$description}",
      "variables": {
        "$srcAbbreHref": {
          "type": "basic",
          "pattern": "<a href='#'>[AAA]</a>",
          "printIfNullValue": true
        },
        "$description": {
          "type": "basic",
          "pattern": "{SubTitle}",
          "printIfNullValue": false
        }
      }
    },
    "right-panel": {
      "read-button": {
        "url": "CardFiles\\CardFiles\\Listing?cardFileId={BookXmlId}"
