{
  "Edition": {
    "middle-panel": {
      "title": "{$printAuthors}{Title}",
      "body": "{PublishPlace}, {PublishDate}, {PublisherText} <br> {$manuscript} <br> Počet stran: {PageCount}",
      "favorites": {
        "maxCount": 5
      },
      "variables": {
        "$test": {
          "type": "if",
          "pattern": "{Century}",
          "onTrue": "{Editor}",
          "onFalse": "was false"
        },
        "$manuscript": {
          "type": "basic",
          "pattern": "{ManuscriptSettlement}, {ManuscriptRepository}, {OriginDate}",
          "printIfNullValue": false
        },
        "$printAuthors": {
          "type": "basic",
          "pattern": "{AuthorsLabel}: ",
          "printIfNullValue": false
        },
        "$authors": {
          "type": "array",
          "scope": "Authors",
          "pattern": "{Name}",
          "delimeter": ", "
        },
        "$litType": {
          "type": "array",
          "scope": "Pages",
          "pattern": "<a href='{BookXmlId}'>[{$LiteraryTypeStart}{$LiteraryTypeEnd}]</a>",
          "delimeter": " , "
        },
        "$LiteraryTypeStart": {
          "type": "basic",
          "pattern": "{Start}",
          "printIfNullValue": false,
          "replaceNullValueBy": ""
        },
        "$LiteraryTypeEnd": {
          "type": "basic",
          "pattern": "-{End}",
          "printIfNullValue": false,
          "replaceNullValueBy": ""
        },
        "$sign": {
          "type": "basic",
          "pattern": ", {Sign}",
          "printIfNullValue": false,
          "replaceNullValueBy": ""
        },
        "$archive": {
          "type": "basic",
          "scope": "Archive",
          "pattern": " {Name} ({City}, {State})",
          "printIfNullValue": false
        },
        "$century": {
          "type": "basic",
          "pattern": ", {Century}. století",
          "printIfNullValue": false
        },
        "$pages": {
          "type": "basic",
          "pattern": ", 5v- 6v, 11v-12v ",
          "printIfNullValue": false
        }
      }
    },
    "bottom-panel": {
      "body": "{$printSearchResultsCondition}{$editionTable}<a href='/EditionNote/EditionNote?bookId={BookId}'>Ediční poznámka</a> <br> Copyright: {Copyright}",
      "variables": {
        "$editionTable": {
          "type": "table",
          "printRowIfNullValue": true,
          "replaceNullValueBy": "&lt;Nezadáno&gt;",
          "rows": [
            {
              "label": "Editor",
              "pattern": "{$editor}"
            },
            {
              "label": "Předloha",
              "pattern": "{$literaryOriginal}"
            },
            {
              "label": "Zkratka památky",
              "pattern": "{RelicAbbreviation}"
            },
            {
              "label": "Zkratka pramene",
              "pattern": "{SourceAbbreviation}"
            },
            {
              "label": "Literární druh",
              "pattern": "{$literaryType}"
            },
            {
              "label": "Literární žánr",
              "pattern": "{$literaryGenre}"
            },
            {
              "label": "Poslední úprava edice",
              "pattern": "{CreateTimeString}"
            }
          ]
        },
        "$printSearchResults": {
          "type": "basic",
          "pattern": "<div class=\"results\"><div class=\"hits-count\">({TotalHitCount}x)</div><div class=\"hits-results\">{$printSearchResultsArray}</div></div>",
          "printIfNullValue": true
        },
        "$printSearchResultsArray": {
          "type": "array",
          "scope": "Results",
          "pattern": "<div class=\"result-hit\">{$printSearchResultContext}</div>",
          "delimeter": ""
        },
        "$printSearchResultContext": {
          "type": "basic",
          "scope": "ContextStructure",
          "pattern": "<div class=\"search-context-before\">{Before}</div> <div class=\"search-context-match\">{Match}</div><div class=\"search-context-after\"> {After}</div>",
          "delimeter": ""
        },
        "$printSearchResultsCondition": {
          "type": "if",
          "pattern": "{Results}",
          "onTrue": "{$printSearchResults}",
          "onFalse": ""
        },

        "$editor": {
          "type": "array",
          "scope": "Editors",
          "pattern": "{FirstName} {LastName}",
          "delimeter": ", "
        },
        "$literaryOriginal": {
          "type": "array",
          "scope": "LiteraryOriginals",
          "pattern": "{$this}",
          "delimeter": ", "
        },
        "$literaryType": {
          "type": "array",
          "scope": "LiteraryKinds",
          "pattern": "{$this}",
          "delimeter": ", "
        },
        "$literaryGenre": {
          "type": "array",
          "scope": "LiteraryGenres",
          "pattern": "{$this}",
          "delimeter": ", "
        }
      }
    },
    "right-panel": {
      "info-button": {
        "onclick": "listBook(this)"
      },
      "read-button": {
        "onclick": "listBook(this)"
      },
      "favorite-button": {},
      "load-detail-url": "Bibliography/GetBookDetailInfo?bookId={BookId}",
      "variables": {
        "$test": {
          "type": "basic",
          "pattern": "{Name}",
          "printIfNullValue": false
        }
      }

    }
  },
  "Dictionary": {
    "middle-panel": {
      "short-info": "{$linked}",
      "title": "{Title} <span class=\"acronym\">[{SourceAbbreviation}]</span>",
      "body": "{BiblText} <br> {PublishPlace}, {PublishDate} <br> {$authors}{$description}{$year}",
      "favorites": {
        "maxCount": 5
      },
      "variables": {
        "$linked": {
          "type": "if",
          "pattern": "{Linked}",
          "onTrue": "zapojený slovník",
          "onFalse": "nezapojený slovník"
        },
        "$authors": {
          "type": "basic",
          "pattern": "{AuthorsLabel}",
          "printIfNullValue": false
        },
        "$description": {
          "type": "basic",
          "pattern": ", {Description}",
          "printIfNullValue": false
        },
        "$year": {
          "type": "basic",
          "pattern": " {OriginDate}.",
          "printIfNullValue": false
        }
      }
    },
    "right-panel": {
      "info-button": {
        "url": "Dictionaries\\Dictionaries\\List?bookId={BookXmlId}"
      },
      "read-button": {
        "url": "Dictionaries\\Dictionaries\\Listing?books=[{BookId}]"
      },
      "favorite-button": {},
      "load-detail-url": "Bibliography/GetBookDetailInfo?bookId={BookId}"
    }
  },

