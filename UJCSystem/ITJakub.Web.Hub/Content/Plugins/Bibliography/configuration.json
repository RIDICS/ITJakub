{
    "Edition": {
        "middle-panel": {
            "title": "Nazev: {Name} {$editor} ,{$litType}", //variable starts with dollar sign
            "body": "{$editionTable} {$pages}",
            "variables": {
                "$litType": {
                    "type": "array",            //could be used only on javascript array object
                    "scope": "LiteraryType", //change scope from bibItem to its child
                    "pattern": "[{$LiteraryTypeStart}{$LiteraryTypeEnd}]", //pattern for each item,changes scope to item
                    "delimeter": "; "
                },
                "$LiteraryTypeStart": {
                    "type": "primitive",
                    "pattern": "{Start}",
                    "printIfNullValue": false,
                    "replaceNullValueBy": ""
                },
                 "$LiteraryTypeEnd": {
                    "type": "primitive",
                    "pattern": "-{End}",
                    "printIfNullValue": false,
                    "replaceNullValueBy": ""
                },
                "$editor": {
                    "type": "primitive",
                    "pattern": ",{Editor}",
                    "printIfNullValue": true,
                    "replaceNullValueBy": ""
                },
                "$editionTable": {
                    "type": "table",
                    "printRowIfNullValue": true,
                    "replaceNullValueBy": "&lt;Nezadáno&gt;", //special characters needs to be escaped
                    "rows": [ { "label": "Editor", "pattern": "{$editor}" }, { "label": "Předloha", "pattern": "{Pattern}" }, { "label": "Zkratka památky", "pattern": "{RelicAbbreviation}" } ]
                }
            }
        },
        "bottom-panel": {
            "custom": "copy: {Copyright}"
        }
    },

    "Dictionary": {
    },

    "OldCzechTextBank": {
    }

}
