{
    "Edition": {
        "middle-panel": {
            "title": "Nazev: {Name}{$editor} {$litType}", 
            "body": "{$editionTable}",
            "variables": {
                "$litType": {
                    "type": "array",           
                    "scope": "Pages", 
                    "pattern": "<a href='{BookId}'>[{$LiteraryTypeStart}{$LiteraryTypeEnd}]</a>", 
                    "delimeter": " , "
                },
                "$LiteraryTypeStart": {
                    "type": "primitive",
                    "pattern": "{Start}",
                    "printIfNullValue": false,
                    "replaceNullValueBy": ""
                },
                 "$LiteraryTypeEnd": {
                    "type": "primitive",
                    "pattern": "-{End}",
                    "printIfNullValue": false,
                    "replaceNullValueBy": ""
                },
                "$editor": {
                    "type": "primitive",
                    "pattern": ", {Editor}",
                    "printIfNullValue": true,
                    "replaceNullValueBy": ""
                },
                "$editionTable": {
                    "type": "table",
                    "printRowIfNullValue": true,
                    "replaceNullValueBy": "&lt;Nezadáno&gt;", 
                    "rows": [ { "label": "Editor", "pattern": "{Editor}" }, { "label": "Předloha", "pattern": "{Pattern}" }, { "label": "Zkratka památky", "pattern": "{RelicAbbreviation}" } ]
                }
            }
        },
        "bottom-panel": {
            "custom": "copy: {Copyright}"
        }
    },

    "Dictionary": {
    },

    "OldCzechTextBank": {
    }

}
