{
  "version": "1.0.0",
  "dependencies": {
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.1",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.1",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.1",
    "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "1.0.0",
    "Microsoft.AspNetCore.Authentication.Cookies": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
    "AutoMapper": "5.2.0",
    "bootstrap.TypeScript.DefinitelyTyped": "0.9.2",
    "Castle.Windsor": "3.3.0",
    "Castle.Windsor.MsDependencyInjection": "1.3.0",
    "dropzone.TypeScript.DefinitelyTyped": "1.3.4",
    "jquery.TypeScript.DefinitelyTyped": "3.1.0",
    "jqueryui.TypeScript.DefinitelyTyped": "1.4.8",
    "log4net": "2.0.7",
    "Log4net.Extensions.Logging": "1.0.0-beta11",
    "Markdig": "0.10.4",
    "Newtonsoft.Json": "9.0.1",
    "NHibernate": "4.1.0.4000",
    "simplemde.TypeScript.DefinitelyTyped": "0.0.1",
    "typeahead.TypeScript.DefinitelyTyped": "0.3.5",
    "System.Runtime.Serialization.Xml": "4.3.0",
    "System.ServiceModel.Http": "4.3.0",
    "System.ServiceModel.Primitives": "4.3.0",
    "DryIoc.dll": "2.9.7",
    "DryIoc.Microsoft.DependencyInjection": "1.1.0"
  },

  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },

  "frameworks": {
    "net461": {
      "dependencies": {
        "ITJakub.ITJakubService.DataContracts": {
          "target": "project"
        },
        "ITJakub.Lemmatization.Shared.Contracts": {
          "target": "project"
        },
        "ITJakub.Shared.Contracts": {
          "target": "project"
        },
        "ITJakub.Web.DataEntities": {
          "target": "project"
        },
        "Vokabular.MainService.DataContracts": {
          "target": "project"
        },
        "Vokabular.Shared": {
          "target": "project"
        }
      },
      "frameworkAssemblies": {}
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "Areas/**/Views/**/*.cshtml",
      "Content",
      "appsettings*.json",
      "globalsettings.json",
      "web.config"
    ]
  },

  "scripts": {
    "prepublish": [ "npm install", "bower install", "gulp clean", "gulp build:ts", "gulp default" ],
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
