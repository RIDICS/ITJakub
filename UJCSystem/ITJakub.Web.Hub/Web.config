<?xml version="1.0"?>
<configuration>
  <configSections/>
  <connectionStrings>
    <add name="ITJakub.Web.Hub.Properties.Settings.DefaultConnectionString" connectionString="Server=localhost;Database=ITJakubWebDB;User Id=admin;Password=***REMOVED***;" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
  -->
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.6" />
    <httpRuntime targetFramework="4.6" maxRequestLength="1048576000" executionTimeout="3600" />
    <roleManager enabled="false" />
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
    <urlCompression doStaticCompression="false" doDynamicCompression="true" dynamicCompressionBeforeCache="false" />
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="1073741824" />
      </requestFiltering>
    </security>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" culture="neutral" publicKeyToken="30ad4fe6b2a6aeed" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Helpers" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-5.2.0.0" newVersion="5.2.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Optimization" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-1.1.0.0" newVersion="1.1.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.WebPages" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="WebGrease" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="0.0.0.0-1.5.2.14234" newVersion="1.5.2.14234" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.serviceModel>
    <client>
      <endpoint name="ItJakubService" address="http://localhost/ITJakub.ITJakubService/ItJakubService.svc" contract="ITJakub.ITJakubService.DataContracts.IItJakubService" binding="basicHttpBinding" bindingConfiguration="ITJakub.BasicHttpBinding" />
      <endpoint name="ItJakubService.Authenticated" address="https://localhost/ITJakub.ITJakubService/ItJakubService.svc" contract="ITJakub.ITJakubService.DataContracts.IItJakubService" binding="basicHttpsBinding" bindingConfiguration="ITJakub.BasicHttpsBinding.UserNameAuthentication" />
      <endpoint name="ItJakubServiceStreamed" address="http://localhost/ITJakub.ITJakubService/ItJakubServiceStreamed.svc" contract="ITJakub.ITJakubService.DataContracts.IItJakubServiceStreamed" binding="basicHttpBinding" bindingConfiguration="ITJakub.BasicHttpBinding.Streamed" />
      <endpoint name="ItJakubServiceStreamed.Authenticated" address="https://localhost/ITJakub.ITJakubService/ItJakubServiceStreamed.svc" contract="ITJakub.ITJakubService.DataContracts.IItJakubServiceStreamed" binding="basicHttpsBinding" bindingConfiguration="ITJakub.BasicHttpsBinding.Streamed" />
      <endpoint name="ItJakubServiceEncrypted" address="https://localhost/ITJakub.ITJakubService/ItJakubServiceEncrypted.svc" contract="ITJakub.ITJakubService.DataContracts.IItJakubServiceEncrypted" binding="basicHttpsBinding" bindingConfiguration="ITJakub.BasicHttpsBinding.CertificateAuthentication" behaviorConfiguration="ITJakub.EndpointBehavior.CertificateAuthentication" />
      <endpoint name="LemmatizationService" address="http://localhost/ITJakub.Lemmatization.Service/LemmatizationService.svc" contract="ITJakub.Lemmatization.Shared.Contracts.ILemmatizationService" binding="basicHttpBinding" />
    </client>
    <bindings>
      <basicHttpBinding>
        <binding name="ITJakub.BasicHttpBinding" maxBufferPoolSize="2147483647" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" closeTimeout="00:10:00" openTimeout="00:10:00" sendTimeout="00:10:00">
          <readerQuotas maxDepth="2000000" maxStringContentLength="2147483647" maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
        </binding>
        <binding name="ITJakub.BasicHttpBinding.Streamed" maxBufferPoolSize="2097120" maxBufferSize="65536" maxReceivedMessageSize="2147483647" closeTimeout="00:25:00" openTimeout="00:25:00" sendTimeout="00:10:00" transferMode="Streamed" />
      </basicHttpBinding>
      <basicHttpsBinding>
        <binding name="ITJakub.BasicHttpsBinding.UserNameAuthentication" maxBufferPoolSize="2147483647" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" closeTimeout="00:10:00" openTimeout="00:10:00" sendTimeout="00:10:00">
          <readerQuotas maxDepth="2000000" maxStringContentLength="2147483647" maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
          <security mode="TransportWithMessageCredential">
            <transport clientCredentialType="Basic" />
          </security>
        </binding>
        <binding name="ITJakub.BasicHttpsBinding.Streamed" maxBufferPoolSize="2097120" maxBufferSize="65536" maxReceivedMessageSize="2147483647" closeTimeout="00:25:00" openTimeout="00:25:00" sendTimeout="00:10:00" transferMode="StreamedRequest">
          <security mode="TransportWithMessageCredential">
            <transport clientCredentialType="Basic" />
          </security>
        </binding>
        <binding name="ITJakub.BasicHttpsBinding.CertificateAuthentication">
          <security mode="Transport">
            <transport clientCredentialType="Certificate" />
          </security>
        </binding>
      </basicHttpsBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="ITJakub.EndpointBehavior.CertificateAuthentication">
          <clientCredentials>
            <clientCertificate findValue="C787F20847606DC40E50E015A5D2E3A9E59B704C" storeLocation="LocalMachine" storeName="My" x509FindType="FindByThumbprint" />
