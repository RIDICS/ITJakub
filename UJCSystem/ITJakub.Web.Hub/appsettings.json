{
  "Endpoints": [
    {
      "Name": "MainService",
      "Address": "https://localhost/Vokabular.MainService/api/"
    },
    {
      "Name": "LemmatizationService",
      "Address": "http://localhost/ITJakub.Lemmatization.Service/LemmatizationService.svc"
    }
  ],
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "OpenIdConnect": {
    "Url": "https://localhost:44395/",
    "ClientId": "Vokabular",
    "ClientSecret": "secret"
  },
  "Localization": {
    "BasePath": "Localization",
    "SupportedCultures": [ "cs-CZ", "en" ],
    "DefaultCulture": "cs-CZ",
    "FirstAutoTranslateResource": "File",
    "AutoLoadResources": true,
    "TranslationFallbackMode": "Exception"
  }, 
  "GoogleCalendar": {
    "CalendarId": "ridics.events@gmail.com" //Attention: Google Calendar must be public https://support.google.com/calendar/answer/41207?hl=en
  } 
}